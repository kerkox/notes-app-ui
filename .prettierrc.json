{
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "useTabs": false,
  "endOfLine": "lf",
  "printWidth": 80,
  "arrowParens": "always",
  "proseWrap": "preserve",
  "htmlWhitespaceSensitivity": "strict",
  "jsxBracketSameLine": false,
  "bracketSpacing": true,
  "insertPragma": false,
  "requirePragma": false,
  "quoteProps": "as-needed",
  "trailingComma": "all",
  "eslintIntegration": true,
  "parser": "typescript",
  "jsxSingleQuote": false,
  "vueIndentScriptAndStyle": false,
  "bracketSameLine": false,
  "plugins": [
    "prettier-plugin-tailwindcss"
  ]  
}